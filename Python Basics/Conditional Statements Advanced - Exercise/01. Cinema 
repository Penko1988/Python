projection = input()
rows = int(input())
columns = int(input())

price = 0

if projection == "Premiere":
    price = 12.00
    total_sum = rows * columns * price
    print(f"{total_sum:.2f}")
elif projection == "Normal":
    price = 7.50
    total_sum = rows * columns * price
    print(f"{total_sum:.2f}")
elif projection == "Discount":
    price = 5.00
    total_sum = rows * columns * price
    print(f"{total_sum:.2f}")