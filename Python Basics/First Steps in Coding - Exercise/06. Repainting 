nylon = int(input())
paint = int(input())
thinner = int(input())
hours = int(input())
nylon_price = 1.50
paint_price = 14.50
thinner_price = 5.00
bags_price = 0.40
materials_sum = (nylon + 2) * nylon_price + \
    paint * 1.1 * paint_price + \
    thinner * thinner_price + \
    bags_price
salery = materials_sum *0.3 *hours
total_sum = materials_sum +salery
print(total_sum)